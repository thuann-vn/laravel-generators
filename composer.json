{
    "name": "thuann-vn/laravel-generators",
    "type": "library",
    "description": "Laravel generators for quickly creating entities.",
    "keywords": [
        "webfactor",
        "laravel",
        "backpack",
        "generators"
    ],
    "homepage": "https://github.com/webfactor/laravel-generators",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Swonke",
            "email": "thomas.swonke@webfactor.de",
            "homepage": "http://webfactor.de",
            "role": "Developer"
        },
        {
            "name": "Oliver Ziegler",
            "email": "oliver.ziegler@webfactor.de",
            "homepage": "http://webfactor.de",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
        "illuminate/database": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
        "php": "~7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Webfactor\\Laravel\\Generators\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webfactor\\Laravel\\Generators\\GeneratorsServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
